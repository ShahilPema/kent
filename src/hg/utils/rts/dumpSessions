dbs="hg19 hg38"

for db in $dbs
do
    sed '/^ *#/d' ../../htdocs/inc/recTrackSets.$db.tab | tawk '{print $3}' |
    while read session
    do
        hgsql hgcentraltest -Ne "select contents from namedSessionDb where sessionName='$session' && userName='View'" |  tr '&' '\n' | sort -u -u > $db/$session
    done
done

